{
  "name": "mvp",
  "version": "1.1.0",
  "description": "AI-powered penetration testing agent with Intelligence Layer and Evaluation Loop",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "worker": "tsc && node dist/worker.js",
    "worker:dev": "tsx src/worker.ts",
    "worker:record": "RECORD_LLM_CALLS=record LLM_CASSETTE_DIR=./logs/cassettes tsx src/worker.ts",
    "worker:replay": "RECORD_LLM_CALLS=replay LLM_CASSETTE_DIR=./logs/cassettes tsx src/worker.ts",
    "test:reset": "bash scripts/test-reset.sh",
    "test:check": "bash scripts/test-reset.sh --check-only"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flashoop/mvp.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/flashoop/mvp/issues"
  },
  "homepage": "https://github.com/flashoop/mvp#readme",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.29",
    "@anthropic-ai/sdk": "^0.72.1",
    "@cyber/mcp-rag-memory-client": "file:.yalc/@cyber/mcp-rag-memory-client",
    "@langfuse/otel": "^4.5.1",
    "@langfuse/tracing": "^4.5.1",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "dotenv": "^17.2.3",
    "ioredis": "^5.9.3",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/ioredis": "^4.28.10",
    "@types/node": "^25.2.0",
    "tsx": "^4.19.2"
  }
}
